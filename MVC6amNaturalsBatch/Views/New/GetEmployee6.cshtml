@model MVC6amNaturalsBatch.Models.EmpDept
@{
    ViewBag.Title = "GetEmployee6";
}

<h2>GetEmployee6</h2>

<table class="table table-bordered table-condensed table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in Model.emp)//3
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>


<table class="table table-bordered table-condensed table-striped">
    <tr>
        <th>Dept Id</th>
        <th>Dept Name</th>
    </tr>
    @foreach (var item in Model.dept)//3
    {
        <tr>
            <td>@item.DeptId</td>
            <td>@item.DeptName</td>
        </tr>
    }
</table>